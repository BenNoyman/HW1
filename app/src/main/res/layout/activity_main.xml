<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/skybackground"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_70dp"
        android:layout_marginBottom="@dimen/plane_margin_bottom"
        android:layout_alignParentBottom="true"
        android:baselineAligned="false"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/LeftPlane"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/plane_left"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/MidPlane"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/plane_mid"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="horizontal">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/RightPlane"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:srcCompat="@drawable/plane_right"
                />
        </LinearLayout>
    </LinearLayout>

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/main_LBL_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:text=""
        android:contentDescription="aadsf"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_margin="@dimen/margin_default"
        app:icon="@drawable/left"
         />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/main_LBL_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:text=""
        android:contentDescription="aadsf"
        app:iconGravity="end"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="@dimen/margin_default"
        app:icon="@drawable/right"
         />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:baselineAligned="false"
        android:orientation="horizontal"

        >

        <!--left side-->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:gravity="center"
            >
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api1"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api4"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api7"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api10"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api13"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />
        </LinearLayout>


        <!--mid-->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api2"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api5"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api8"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api11"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api14"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />
        </LinearLayout>

        <!--right side-->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api3"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api6"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api9"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api12"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_weight="1"
                android:layout_gravity="center"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main_LBL_api15"
                android:layout_width="75dp"
                android:layout_height="75dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:layout_margin="@dimen/margin_default"
                android:layout_marginLeft="@dimen/margin_default"
                app:srcCompat="@drawable/astroid" />
        </LinearLayout>

    </LinearLayout>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/main_IMG_hearts"
        android:layout_width="@dimen/hearts_width"
        android:layout_height="@dimen/heart_dimention"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="@dimen/margin_default"
        android:layout_marginTop="16dp">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/main_IMG_heart1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/heart" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/main_IMG_heart2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_10dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/heart" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/main_IMG_heart3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/heart" />
    </androidx.appcompat.widget.LinearLayoutCompat>

</RelativeLayout>